{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "error",
    "semi": ["warn", "always"],
    "quotes": ["warn", "double"],
    "indent": ["warn", 2],
    "no-multiple-empty-lines": ["warn", { "max": 1 }]
  }
}
